---
interface Props {
	title: string;
}

const { title } = Astro.props;
---


<!doctype html>
<html lang="es">
	<head>
		<meta charset="UTF-8" />
		<meta name="description" content="Astro description" />
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/png" href="/logo/logo42.png"/>
		<meta name="generator" content={Astro.generator} />
		<title>{title}</title>
	</head>
	<body>
		<slot />
	</body>
</html>
<style is:global>
	:root {

	}
	html {
		font-family: "Quicksand Variable, sans-serif;";
		background: black;
		background-size: 100%;
	}
</style>
